{
  "name": "fed-tictactoe-vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://nanidam.github.io/tic-tac-toe/",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "pinia": "^2.1.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.vue",
        "options": {
          "parser": "vue"
        }
      }
    ]
  }
}
